<launch4jConfig>
    <headerType>0</headerType>
    <jar>../../dist/hipster.jar</jar>
    <outfile>../../application/hIPster.exe</outfile>
    <errTitle>hIPster</errTitle>
    <chdir>.</chdir>
    <customProcName>true</customProcName>
    <icon>hipster.ico</icon>
    <jre args='-Dlaunch4j.exedir="%EXEDIR%"'>
        <minVersion>1.5.0</minVersion>
    </jre>
    <splash>
        <file>hipster_splash.bmp</file>
        <waitForWindow>true</waitForWindow>
        <timeout>60</timeout>
        <timeoutErr>true</timeoutErr>
    </splash>
</launch4jConfig>